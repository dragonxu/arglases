<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="1px"/>
            <solid android:color="#fff"/>
            <padding
                android:left="260px"

                android:right="260px"
               />
            <size
                android:width="14px"
                android:height="14px"/>
        </shape>
    </item>

    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="90px"/>
                <solid android:color="#fff"/>
                <padding
                    android:left="260px"

                    android:right="260px"
                    />
                <size
                    android:width="14px"
                    android:height="14px"/>
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="1px"/>
                <solid android:color="#fff" />
                <padding
                    android:left="260px"

                    android:right="260px"
                    />
<size
    android:width="14px"
    android:height="14px"/>
            </shape>
        </clip>
    </item>
</layer-list>
